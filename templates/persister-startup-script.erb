# Startup script for the <%= persister_service_name %>

description "<%= persister_service_name %> java app"
start on runlevel [2345]

console log
respawn

setgid monasca
setuid persister
exec /usr/bin/java -Dfile.encoding=UTF-8 -Xmx8g -cp /opt/monasca/monasca-persister.jar monasca.persister.PersisterApplication server /etc/monasca/<%= persister_service_name %>.yml
