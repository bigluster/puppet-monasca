---
init_config:
  admin_password: <%= @admin_password %>
  admin_tenant_name: <%= @admin_tenant_name %>
  admin_user: <%= @admin_user %>
  identity_uri: <%= @identity_uri %>
<%- if @region_name -%>
  region_name: <%= @region_name %>
<%- end -%>
  cache_dir: <%= @cache_dir %>
  nova_refresh: <%= @nova_refresh %>
<%- if @heat_scale_group -%>
  metadata:
  - scale_group
<%- end -%>
  vm_probation: <%= @vm_probation %>

instances:
 - {}
