 - name: <%= @title %>
   check_command: <%= @check_command %>
<%- if @check_name -%>
   metric_name: <%= @check_name %>
<%- end -%>
<%- if @host_name -%>
   host_name: <%= @host_name %>
<%- end -%>
<%- if @check_interval -%>
   check_interval: <%= @check_interval %>
<%- end -%>
<%- if @dimensions -%>
   dimensions: 
<%- @dimensions.each do |key, value| -%>
      <%= key %>: <%= value %>
<%- end -%>
<%- end -%>
