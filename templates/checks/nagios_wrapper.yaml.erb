---
init_config:
  check_path: <%= check_path %>
  temp_file_path: <%= temp_file_path %>

instances:
<% instances.each do |instance| -%>
<% for index in 0 ... instance.size -%>
<% if index == 0 -%>
 - <%= instance.keys[index] %>: <%= instance.values[index] %>
<% else -%>
   <%= instance.keys[index] %>: <%= instance.values[index] %>
<% end -%>
<% end -%>
<% end -%>