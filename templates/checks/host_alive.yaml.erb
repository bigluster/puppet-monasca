---
init_config:
  ssh_port: <%= ssh_port %>
  ssh_timeout: <%= ssh_timeout %>
  ping_timeout: <%= ping_timeout %>

instances:
<% instances.each do |instance| -%>
<% for index in 0 ... instance.size -%>
<% if index == 0 -%>
 - <%= instance.keys[index] %>: <%= instance.values[index] %>
<% else -%>
   <%= instance.keys[index] %>: <%= instance.values[index] %>
<% end -%>
<% end -%>
<% end -%>